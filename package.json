{
  "name": "cocumove",
  "version": "1.0.0",
  "main": "server/index.ts",
  "type": "module",
  "description": "Plataforma de transporte tipo BOLT con sistema de recompensas en COCU TOKENS",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "tsx watch server/index.ts",
    "client:dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "db:generate": "drizzle-kit generate:pg",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push:pg",
    "db:studio": "drizzle-kit studio",
    "lint": "eslint src server --ext .ts,.tsx",
    "type-check": "tsc --noEmit",
    "test": "vitest"
  },
  "keywords": [
    "transportation",
    "ride-sharing",
    "tokens",
    "blockchain"
  ],
  "author": "COCUMOVE Team",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.916.0",
    "@aws-sdk/s3-request-presigner": "^3.916.0",
    "@types/better-sqlite3": "^7.6.13",
    "@vitejs/plugin-react": "^5.1.0",
    "axios": "^1.12.2",
    "bcrypt": "^6.0.0",
    "better-sqlite3": "^12.4.1",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.5",
    "drizzle-orm": "^0.44.7",
    "express": "^5.1.0",
    "express-validator": "^7.3.0",
    "i18next": "^25.6.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "pg": "^8.16.3",
    "postgres": "^3.4.7",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-i18next": "^16.2.0",
    "uuid": "^13.0.0",
    "vite": "^7.1.12"
  },
  "devDependencies": {
    "@types/express": "^5.0.4",
    "@types/node": "^24.9.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "concurrently": "^8.2.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
